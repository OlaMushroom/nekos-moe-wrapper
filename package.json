{
  "name": "nekos-moe",
  "version": "0.7.1",
  "description": "JS/TS wrapper for Nekos.moe API.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlaMushroom/nekos-moe-wrapper.git"
  },
  "license": "MIT",
  "author": "OlaMushroom",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./index.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "nekos": "bin/cli.js"
  },
  "files": [
    "index.ts",
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "bun build.js && bun dts-bundle-generator index.ts -o lib/index.d.ts --no-banner",
    "compile": "bun build bin/cli.js --minify --sourcemap --compile --outfile nekos"
  },
  "dependencies": {
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bun": "latest",
    "dts-bundle-generator": "^9.5.1",
    "typescript": "^5.5.3"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
