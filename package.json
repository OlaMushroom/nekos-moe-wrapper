{
  "name": "nekos-moe",
  "version": "0.6.2",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/OlaMushroom/nekos-moe-wrapper.git"
  },
  "type": "module",
  "module": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "bin"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "bin": {
    "nekos": "./bin/cli.js"
  },
  "scripts": {
    "bundle": "tsc --outDir lib && bun build.js",
    "bin": "bun build --compile --minify --sourcemap ./bin/cli.js",
    "compile": "bun bin --outfile nekos",
    "compile:linux": "bun bin --target=bun-linux-x64 --outfile ./compiled/nekos-linux",
    "compile:win": "bun bin --target=bun-windows-x64 --outfile ./compiled/nekos-win",
    "compile:mac": "bun bin --target=bun-darwin-x64 --outfile ./compiled/nekos-mac",
    "compile:mac-arm": "bun bin --target=bun-darwin-arm64 --outfile ./compiled/nekos-mac-arm",
    "compile:all": "bun npm-run-all -s compile:*"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bun": "^1.1.6",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "typescript": "^5.5.3"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}